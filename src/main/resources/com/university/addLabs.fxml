<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.university.AddLabsController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" layoutX="144.0" layoutY="41.0" prefHeight="21.0" prefWidth="337.0" style="&#10;" text="Add Labs" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Button id="addComputersButton" fx:id="addComputersButton" layoutX="195.0" layoutY="330.0" mnemonicParsing="false" onAction="#addComputersButtonClicked" prefHeight="30.0" prefWidth="250.0" text="Add Computers" />
            <VBox id="fieldsHolder" fx:id="fieldsHolder" layoutX="19.0" layoutY="87.0" prefHeight="226.0" prefWidth="588.0" spacing="10.0" style="-fx-border-color: #ccc;">
               <children>
                  <TextField id="labNameField" fx:id="labNameField" layoutX="21.0" layoutY="21.0" prefHeight="40.0" prefWidth="250.0" promptText="Lab Name" />
                  <TextField id="inchargeNameField" fx:id="inchargeNameField" prefHeight="40.0" prefWidth="200.0" promptText="Incharge Name" />
                  <TextField id="inchargeGradeField" fx:id="inchargeGradeField" layoutX="21.0" layoutY="71.0" prefHeight="40.0" prefWidth="566.0" promptText="Incharge Grade" />
                  <CheckBox id="hasProjectorCheck" fx:id="hasProjectorCheck" mnemonicParsing="false" text="Has Projector" />
                  <Button id="addMoreDepButton" fx:id="addMoreDepButton" mnemonicParsing="false" onAction="#addMoreDepButtonClicked" text="Add More" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</VBox>
